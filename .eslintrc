{
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "plugins": ["jest"],
    "rules": {
        "max-len": [
            2,
            {
                "code": 140,
                "tabWidth": 4,
                "ignoreUrls": true
            }
        ],
        "require-jsdoc" : 0
    },
    "env": {
        "browser": true,
        "node": true,
        "jest/globals": true
    }
}